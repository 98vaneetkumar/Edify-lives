<!DOCTYPE html>
<html class="loading" lang="en" data-textdirection="ltr">
  <!-- BEGIN: Head-->

  <%-include('../../common/header')%>
  <!-- BEGIN: Body-->

  <body
    class="vertical-layout vertical-menu-modern navbar-floating footer-static"
    data-open="click"
    data-menu="vertical-menu-modern"
    data-col=""
  >
    <!-- sidebar -->
    <%-include('../../common/sidebar')%>
    <!-- navbar -->

    <%-include('../../common/navbar')%>
    <!-- BEGIN: Content-->
    <div class="app-content content">
      <div class="content-overlay"></div>
      <div class="header-navbar-shadow"></div>
      <div class="content-wrapper">
        <div class="content-header row"></div>

        <div class="content-body">
          <div class="row justify-content-center">
            <!-- <div class="col-md-6"> -->
            <div class="content-header-left col-md-12 col-12 mb-2">
              <div class="auth-wrapper auth-v1 px-2">
                <div class="auth-inner py-2">
                  <!-- Register v1 -->
                  <div class="card mb-0">
                    <div class="card-body">
                      <a href="/admin/business_listing" class="btn btn-primary">
                        <i
                          class="ph ph-arrow-fat-lines-left"
                          style="font-size: 20px"
                        ></i>
                      </a>
                      <a href="#" class="brand-logo">
                        <div
                          style="
                            display: flex;
                            justify-content: center;
                            align-items: center;
                          "
                        >
                          <% if(!data.businessLogo) { %>
                          <img
                            src="../../assets/images (1).jpeg"
                            class="profile_img"
                            alt="avatar"
                            height="120px"
                            width="150px"
                            style="align-items: center"
                          />
                          <% } else { %>
                          <img
                            src="<%= data.businessLogo %>"
                            class="profile_img"
                            alt="user image"
                            height="120px"
                            width="150px"
                          />
                          <% } %>
                        </div>
                      </a>
                      <form class="auth-register-form mt-2">
                        <div class="mb-1">
                          <label for="businessName" class="form-label"
                            >Business Name</label
                          >
                          <input
                            type="text"
                            class="form-control"
                            id="businessName"
                            name="businessName"
                            aria-describedby="businessName"
                            value="<%=data.businessName ? data.businessName : 'N/A' %>"
                            disabled
                          />
                        </div>
                        <div class="mb-1">
                          <label for="typeOfBusiness" class="form-label"
                            >Type Of Business</label
                          >
                          <input
                            type="text"
                            class="form-control"
                            id="typeOfBusiness"
                            name="typeOfBusiness"
                            value="<%=data.typeOfBusiness ? data.typeOfBusiness : 'N/A' %>"
                            disabled
                          />
                        </div>
                        <div class="mb-1">
                          <label for="businessAddress" class="form-label"
                            >Business Address</label
                          >
                          <input
                            type="text"
                            class="form-control"
                            id="businessAddress"
                            name="businessAddress"
                            value="<%=data.businessAddress ? data.businessAddress : 'N/A' %>"
                            disabled
                          />
                        </div>
                        <div class="mb-1">
                          <label for="businessUserAddress" class="form-label"
                            >Business User Address</label
                          >
                          <input
                            type="businessUserAddress"
                            class="form-control"
                            id="businessUserAddress"
                            name="businessUserAddress"
                            value="<%=data.businessUserAddress ? data.businessUserAddress : 'N/A' %>"
                            disabled
                          />
                        </div>

                        <div class="mb-1">
                          <label for="maritalStatus" class="form-label"
                            >Marital Status</label
                          >
                          <input
                            type="maritalStatus"
                            class="form-control"
                            id="maritalStatus"
                            name="maritalStatus"
                            value="<%=data.maritalStatus ? data.maritalStatus : 'N/A' %>"
                            disabled
                          />
                        </div>
                        <div class="mb-1">
                          <label for="location" class="form-label"
                            >Location</label
                          >
                          <input
                            type="location"
                            class="form-control"
                            id="location"
                            name="location"
                            value="<%=data.location ? data.location : 'N/A' %>"
                            disabled
                          />
                        </div>
                        <div class="mb-1">
                          <label for="donateEdifyLivers" class="form-label"
                            >Donate Edify Live</label
                          >
                          <input
                            type="donateEdifyLivers"
                            class="form-control"
                            id="donateEdifyLivers"
                            name="donateEdifyLivers"
                            value="<%=data.donateEdifyLivers ? data.donateEdifyLivers : 'N/A' %>"
                            disabled
                          />
                        </div>
                      </form>
                      <br /><br />
                    </div>
                  </div>

                  <!-- /Register v1 -->
                </div>
              </div>
            </div>
            <!-- </div> -->
          </div>
        </div>
      </div>
    </div>
    <!-- END: Content-->

    <%-include('../../common/script')%>

    <script>
      $(window).on("load", function () {
        if (feather) {
          feather.replace({
            width: 14,
            height: 14,
          });
        }
      });
    </script>
  </body>
  <!-- END: Body-->
</html>
