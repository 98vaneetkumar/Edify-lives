  <% if(msg != '' || error !='' ) { %>
  <div class="alert alert-success msg" style="position: absolute; z-index: 9999; color:black; background-color: white;top: 8%; left: 50%;text-align: center;">
    <% if(msg !='' ) { %>
    <span style="font-size: 15px; color: white; background-color:  #40bf40
            ; padding: 10px;">
      <%= msg %>
    </span>
    <% } else { %>
    <span style="font-size: 15px; color: white; background-color:  red
            ; padding: 10px; ">
      <%= error %>
    </span>
    <% } %>
  </div>
  <% } %>

  <script>
    setTimeout(function() {
      $("div.msg").fadeOut();
    }, 2000);
  </script>
